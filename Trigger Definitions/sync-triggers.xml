                        <Parameter>True</Parameter>
                        <Parameter>1</Parameter>
                        <Parameter />
                    </Parameters>
                </Action>
                <Action>
                    <TypeGuid>Iq135Bd4Tu2ZtFcdArOtTQ==</TypeGuid>
                    <Parameters>
                        <Parameter>{CMD:/wsl "$(wslpath "{DB_DIR}\.admin\.scripts\get-import-dir.sh")"/M=C,WS=H/}\{DB_NAME}</Parameter>
